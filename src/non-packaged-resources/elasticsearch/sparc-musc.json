[
	{
		"database": "cd2h",
		"index": "cd2h-sparc-musc",
		"plugins": [],
		"nodes": {
			"table": "service_raw",
			"schema": "sparc_musc",
			"columns": [
				"id",
				"raw->name",
				"raw->direct_link",
				"raw"
			],
			"transform": {
				"rename": {
					"raw_name": "label",
					"raw_direct_link": "url"
				},
				"concat": {
                    "columns": [ "'http://'", "url"],
                    "destination": "full_url"
                }
				
			}
		}
	}
]
